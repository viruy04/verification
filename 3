#include <stdio.h>
int main() {
    float arr[] = {-3 , 23, -324, -9, 4}; 
    int size = sizeof(arr) / sizeof(arr[0]);
    float sum = 0;
    for(int i = 0; i < size; i++) {
        if(arr[i] < 0) {
            sum += arr[i] * arr[i];
        }
    }
    printf("Сумма квадратов отрицательных элементов: %.0f\n", sum);
    return 0;
}

#include <stdio.h>
int main() {
    int arr[] = {63, -5, 17, 6, -3}; 
    int size = sizeof(arr) / sizeof(arr[0]);
    int count = 0;
    for(int i = 0; i < size; i++) {
        if(arr[i] < 0) {
            count++;
        }
    }
    printf("Количество отрицательных элементов: %d\n", count);
    return 0;
}
